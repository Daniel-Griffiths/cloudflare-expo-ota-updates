  },
  "devDependencies": {
    "@types/node": "^24.7.0",
    "@types/yargs": "^17.0.33",
    "@vitest/ui": "^3.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/cloudflare-expo-ota-updates.git",
    "directory": "packages/deploy"
